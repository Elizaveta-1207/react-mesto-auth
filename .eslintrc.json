{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base",
        "plugin:react/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": 0,
      "no-underscore-dangle": ["error", {
         "allow": ["_id"]
      }],
        "quotes": ["error", "double"],
        "react/prop-types": 0,
        "no-shadow": 0,
        "comma-dangle": 0
    }
}